{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "max-len": [1, 120],
    "arrow-body-style":[0],
    "no-param-reassign": [0],
    "no-console": [0],
    "no-alert": [0],
    "react/sort-comp": [0],
    "object-curly-spacing": [0],
    "curly": [0],
    "prefer-template": [0],
    "react/prefer-es6-class": [0],
    "react/jsx-no-bind": [0],
    "react/jsx-indent-props": [0]
  }
}